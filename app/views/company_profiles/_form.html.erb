<%= simple_form_for [@company_profile] do |f| %>
  <%= f.input :name %>
  <%= f.input :uen %>
  <%= f.input :address %>
  <%= f.input :url %>
  <%= f.input :paid_up_capital %>
  <%= f.input :last_fy_revenue %>
  <%= f.input :legal_disputes %>
  <%= f.input :acra, as: :file %>
  <%= f.input :financial_statement, as: :file %>
  <%= f.input :relevant_licences, as: :file %>
  <%= f.submit %>
<% end %>
