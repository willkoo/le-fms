<h1>Manage All Quizzes</h1>

<ul>
  <% @quizzes.each do |quiz| %>
    <li><%= quiz.id %></li>
    <li><%= link_to "#{quiz.quiz_name}", franchise_quiz_path(quiz, quiz.franchise) %></li>
    <li><%= quiz.franchise.name %></li>
    <li><%= quiz.video_url %></li>
    <li><%= quiz.video_completed %></li><br>
  <% end %>
</ul>
